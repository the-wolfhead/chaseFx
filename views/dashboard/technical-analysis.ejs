              <script> 
            function rat(){
                swal({
                    text: 'Please input your phone number with country code and we will immediately contact you.',
                    content: "input",
                    icon: 'warning',
                    button: true,
                })
                .then((value) =>{
                    !value ? 
                    swal({
                    text: " No phone number entered ! ",
                    icon: "error"
                    })
                    :
                    value.length < 10 ? 
                        swal({
                        text: " Invalid phone number! ",
                        icon: "error"
                        })
                    :   
                    swal({
                        text: " Thank You, we will get in touch with you shortly ",
                        icon: "success"
                        })

                
                });
            }

